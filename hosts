[all:vars]
# Note this becomes the bigiq web credentials
ansible_user=admin
# Leave admin password set to default for AWS and Azure
ansible_ssh_pass=admin
ansible_port=22

[f5-bigiq:children]
f5-bigiq-cm
f5-bigiq-dcd

[f5-bigiq-cm]
# Remove discoveryip if you want to use the management IP as discovery address (recommended for AWS and Azure deployment)
# discoveryip should include a mask as shown in the example below (this will create a self-ip on the BIG-IQ)
# The hostname must be a Fully Qualified Domain Name
bigiq-cm-1.lab.local ansible_host=10.192.75.96 discoveryip=10.192.75.96/24 node_type=cm haprimary=True license=<license>

[f5-bigiq-dcd]
bigiq-dcd-1.lab.local ansible_host=10.192.75.97 discoveryip=10.192.75.97/24 node_type=dcd license=<license>

[f5-bigiq:vars]
bigiq_masterkey=ThisIsMySuperSecretMasterKey123!
bigiq_ntp_server=time.google.com
bigiq_dns_server=8.8.8.8
bigiq_dns_search=example.local
bigiq_timezone=America/Los_Angeles
# the values below are the default values for admin and root accounts that need to be changed. 
# the new admin and root password value is the value of ansible_ssh_pass under [all:vars].
# Do not change the values for bigiq_default_admin_pwd or bigiq_default_root_pwd below.
bigiq_default_admin_pwd=admin
bigiq_default_root_pwd=default
